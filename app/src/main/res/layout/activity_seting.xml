<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.SetingActivity.SetingActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <Button
            android:id="@+id/login"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/login" />
        <Button
            android:id="@+id/change_language"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/changeLanguage" />

        <Button
            android:id="@+id/to_testactivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="进入测试页面" />

        <Button
            android:id="@+id/show_all_events"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="显示所有事件" />

        <Button
            android:id="@+id/show_all_temporals"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="显示所有冲突" />

        <Button
            android:id="@+id/upload_a_event"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="上传一个事件" />

        <Button
            android:id="@+id/upload_all_events"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="上传所有事件" />

        <Button
            android:id="@+id/upload_a_incon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="上传一个冲突" />

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>